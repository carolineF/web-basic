<% if(multipleChoice.length > 0){ %>
<section class='form-group row panel panel-default'>
    <header class="panel-heading"><h3>三、多选题</h3></header>
    <div class="panel-body">
        <ol>
        <% for(var i = 0; i < multipleChoice.length; i++){ %>
        <li>
            <header><%= multipleChoice[i].question%></header>
            <ol type="A" class="list-unstyled">
                <% var array = ['A', 'B', 'C', 'D'];
                  var options =  multipleChoice[i].options.split(',');
                %>
                <% for(var x = 0; x < options.length; x++){ %>
                <li class="checkbox"><label>
                  <input type="checkbox" name="<%= multipleChoice[i].name%>" value="<%= array[x]%>"
                    <% if(multipleChoice[i].input && multipleChoice[i].input.indexOf(array[x]) !== -1){ %>
                           checked="checked" <% } %>>
                        <%= options[x]%></label></li>
                <% } %>
            </ol>
        </li>
        <% } %>
    </ol>
    </div>
</section>
<% }%>
